{
  "manifest": {
    "publishingInformation": {
      "locales": {
        "en-US": {
          "name": "ZenKit List Sync",
          "summary": "Unofficial ZenKit Alexa Shopping List Synchronization Skill",
          "description": "This skill provides Alexa shopping list synchronization with a todo.zenkit.com shopping list with the same name. It will create a zenkit shopping list if one doesn't exist. Right now, this only supports syncing the shopping list, but hoping to expand soon.",
          "largeIconUri": "https://raw.githubusercontent.com/"
        }
      },
      "isAvailableWorldwide": false,
      "category": "ORGANIZERS_AND_ASSISTANTS",
      "distributionMode": "PRIVATE",
      "distributionCountries": [
        "US"
      ]
    },
    "apis": {
      "householdList": {}
    },
    "manifestVersion": "1.0",
    "permissions": [
      {
        "name": "alexa::household:lists:read"
      },
      {
        "name": "alexa::household:lists:write"
      }
    ],
    "privacyAndCompliance": {
      "allowsPurchases": false,
      "isExportCompliant": true,
      "containsAds": false,
      "isChildDirected": false,
      "usesPersonalInfo": false
    },
    "events": {
      "endpoint": {
        "uri": "https://fdadd41d.ngrok.io",
        "sslCertificateType": "Wildcard"
      },
      "subscriptions": [
        {
          "eventName": "SKILL_DISABLED"
        },
        {
          "eventName": "SKILL_PERMISSION_ACCEPTED"
        },
        {
          "eventName": "SKILL_PERMISSION_CHANGED"
        },
        {
          "eventName": "ITEMS_CREATED"
        },
        {
          "eventName": "ITEMS_UPDATED"
        },
        {
          "eventName": "ITEMS_DELETED"
        }
      ]
    }
  }
}
