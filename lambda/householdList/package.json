      "json",
      "html"
    ]
  },
  "dependencies": {
    "ask-sdk-core": "^2.7.0",
    "ask-sdk-model": "^1.25.1",
    "aws-sdk": "^2.814.0",
    "exponential-backoff": "^3.1.0",
    "import": "0.0.6",
    "lodash": "^4.17.19",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.8",
    "zenkitsdk": "^1.0.2",
    "ask-sdk-dynamodb-persistence-adapter": "^2.7.0"
  },
  "devDependencies": {
    "aws-lambda-mock-context": "^3.2.1",
    "aws-sdk-mock": "^5.1.0",
    "chai": "^4.2.0",
    "mocha": "^9.2.0",
    "nock": "^13.2.2",
    "nyc": "^15.1.0"
  }
